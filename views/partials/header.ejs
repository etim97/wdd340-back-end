<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page" class="logo-text">ENSIGN MOTORS</a>
  </span>

  <div id="tools">
    <% if (locals.loggedin) { %>
      <span>Welcome <%= locals.accountData.account_firstname %></span> |
      <a href="/account/" title="Go to your account">My Account</a> |
      <a href="/account/logout" title="Click to log out">Logout</a>
    <% } else { %>
      <a title="Click to log in" href="/account/login">My Account</a>
    <% } %>
  </div>
</header>
